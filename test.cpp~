#include<iostream>

using namespace std;

int main(){
  // make boolean array for first 3 rows
  bool grid[3][12];
  for(int i=0;i<2;i+=1){
    for(int j=0;j<12;j+=1){
      grid[i][j] = 1;
    }
  }
  for(int k=0;k<8;k+=1){
    grid[2][k] = 1;
  }
  grid[2][9] = 0;
  grid[2][10] = 1;
  grid[2][11] = 1;

  cout << grid[0][:] << endl;

}
